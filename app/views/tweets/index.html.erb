<h1>Your Tweets</h1>
<%=link_to "Sign Out", destroy_user_session_path, method: :delete, class: "btn btn-default" %>

<table>
  <thead>
    <tr>
      <th>Message</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @tweets.each do |x| %>
      <tr>
        <td><%= x.message %></td>
        <td><%= x.user.username %></td>
        <td><%= link_to ' Show |', x %></td>
        <td><%= link_to 'Edit |', edit_tweet_path(x) %></td>
        <td><%= link_to 'Delete', x, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Tweet', new_tweet_path %>